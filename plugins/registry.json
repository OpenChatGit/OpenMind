{
  "version": "1.0.0",
  "name": "OpenMind API Plugins",
  "description": "Native JavaScript/Python plugins for OpenMind",
  "repository": "https://github.com/OpenChatGit/OpenMindLabs-Plugins",
  "pluginsPath": "Plugins/",
  "iconsBaseUrl": "https://raw.githubusercontent.com/OpenChatGit/OpenMindLabs-Plugins/main/Plugins/icons/",
  "plugins": [
    {
      "id": "openmind-deep-search",
      "name": "DeepSearch",
      "description": "AI-powered web search with real-time results. Search the internet, get current information, and find answers using multiple search engines.",
      "version": "1.0.0",
      "author": "OpenMindLabs",
      "category": "ai",
      "icon": "radar",
      "iconUrl": "radar-white.png",
      "iconUrlDark": "radar-white.png",
      "iconUrlLight": "radar-black.png",
      "type": "native",
      "official": true,
      "main": "index.js",
      "path": "deep-search",
      "provides": ["deep-search-api", "web-search-api", "ui-api"],
      "ui": {
        "slots": {
          "chat-input-left": {
            "type": "toggle-button",
            "id": "deep-search-toggle",
            "icon": "Radar",
            "tooltip": "Toggle DeepSearch Mode",
            "stateKey": "deepSearchEnabled",
            "priority": 90
          }
        }
      },
      "settings": {
        "preferLocalSearxng": true,
        "maxResults": 6,
        "timeout": 10000
      }
    },
    {
      "id": "openmind-image-gen",
      "name": "Image Generation",
      "description": "Generate images from text prompts using local Stable Diffusion models. Supports SDXL-Turbo, SD 1.5 and custom models.",
      "version": "1.0.0",
      "author": "OpenMindLabs",
      "category": "media",
      "icon": "image",
      "iconUrl": "Image-gen.png",
      "iconUrlDark": "Image-gen.png",
      "iconUrlLight": "Image-gen.png",
      "type": "native",
      "official": true,
      "main": "index.js",
      "path": "image-gen",
      "provides": ["image-gen-api", "ui-api"],
      "requirements": {
        "python": ">=3.8",
        "packages": ["torch", "diffusers", "transformers", "accelerate"]
      },
      "ui": {
        "slots": {
          "chat-input-left": {
            "type": "toggle-button",
            "id": "image-gen-toggle",
            "icon": "Image",
            "tooltip": "Toggle Image Generation Mode",
            "stateKey": "imageGenEnabled",
            "priority": 80
          }
        }
      },
      "settings": {
        "defaultWidth": 512,
        "defaultHeight": 512,
        "defaultSteps": 20,
        "defaultGuidance": 7.5
      },
      "models": [
        {
          "id": "sdxl-turbo",
          "name": "SDXL-Turbo",
          "hfModelId": "stabilityai/sdxl-turbo",
          "steps": 4,
          "guidance": 0.0,
          "default": true
        },
        {
          "id": "sd-1.5",
          "name": "Stable Diffusion 1.5",
          "hfModelId": "runwayml/stable-diffusion-v1-5",
          "steps": 20,
          "guidance": 7.5
        }
      ]
    }
  ],
  "categories": [
    { "id": "media", "name": "Media", "icon": "image", "description": "Image, video, and audio generation" },
    { "id": "ai", "name": "AI & ML", "icon": "brain", "description": "Language models and AI tools" },
    { "id": "voice", "name": "Voice", "icon": "mic", "description": "Speech-to-text and text-to-speech" },
    { "id": "data", "name": "Data", "icon": "database", "description": "Databases and data processing" },
    { "id": "tools", "name": "Tools", "icon": "wrench", "description": "Utilities and integrations" }
  ]
}
